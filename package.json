{
  "name": "nadvisor-chatui",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@types/node": "^18.19.31",
    "@types/react": "^18.2.19",
    "@types/react-dom": "^18.2.23",
    "passio-nutrition-advisor-client": "^1.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-scripts": "5.0.1",
    "tailwind": "^4.0.0",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "wt": "npx tailwindcss -i src/index.css -o ./src/tailwind.css --watch",
    "bt": "npx tailwindcss src/index.css -o src/tailwind.css --minify",
    "build": "source .env.development && eval $BUILD",
    "build:dev": "source .env.development && eval $BUILD_DEV",
    "build:production": "source .env.development && eval $BUILD_PRODUCTION",
    "deploy-dev": "source .env.development && eval $DEPLOY_DEV",
    "deploy-prod": "source .env.development && eval $DEPLOY_PROD",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "devDependencies": {
    "@tailwindcss/forms": "latest",
    "@tailwindcss/typography": "latest",
    "autoprefixer": "^10.4.19",
    "tailwindcss": "latest"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
{
  "name": "nadvisor-chatui",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@types/node": "^18.19.31",
    "@types/react": "^18.2.19",
    "@types/react-dom": "^18.2.23",
    "passio-nutrition-advisor-client": "^1.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.23.1",
    "react-scripts": "5.0.1",
    "tailwind": "^4.0.0",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "sh -ac '. ./.env.${REACT_APP_ENV}; yarn run build:tailwind && react-scripts build'",
    "build:dev": "REACT_APP_ENV=development yarn run build",
    "watch:css": "npx tailwindcss -i src/index.css -o ./src/tailwind.css --watch",
    "build:production": "REACT_APP_ENV=production yarn run build",
    "build:tailwind": "npx tailwindcss src/index.css -o src/tailwind.css --minify",
    "deploy-dev": "yarn build:dev && gcloud config set project passio-dev-env && gsutil -m -h \"Cache-Control:private, max-age=0, no-transform\" rsync -r build gs://passio-nutrition-advisor-dev",
    "deploy-prod": "yarn build:production && gcloud config set project passio-prod-env && gsutil -m rsync -r build gs://passio-nutrition-advisor-demo",
    "test": "npx playwright test",
    "eject": "react-scripts eject",
    "format": "prettier --write \"**/*.{js,jsx,json,md,tsx}\"",
    "lint": "eslint ./",
    "lint-fix": "eslint ./ --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@tailwindcss/forms": "latest",
    "@tailwindcss/typography": "latest",
    "autoprefixer": "^10.4.19",
    "tailwindcss": "^3.4.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}